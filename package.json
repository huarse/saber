{
  "name": "@irim/saber",
  "version": "1.2.1",
  "description": "A toolbox for http connect",
  "repository": {
    "type": "git",
    "url": "git@github.com:huarse/saber.git"
  },
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "distName": "Saber",
  "scripts": {
    "lint": "eslint --fix --ext .js,.ts ./src",
    "build:lib": "tsc --outDir lib --module CommonJS --target ES5",
    "build:esm": "tsc --outDir esm --module ES6 --target ES2018",
    "build:dist": "rollup -c ./config/rollup.config.js",
    "build": "npm run build:lib && npm run build:esm && npm run build:dist",
    "prebuild": "npm run lint && npm run clear",
    "dev": "tsc --outDir lib --module CommonJS --target ES2018 --watch",
    "clear": "rm -rf dist lib esm types",
    "test": "jest --config=config/jest.config.js",
    "test:watch": "jest --config=config/jest.config.js --watch --silent",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "CAIHUAZHI",
    "email": "huarse@gmail.com"
  },
  "maintainers": [
    {
      "name": "CAIHUAZHI",
      "email": "huarse@gmail.com"
    }
  ],
  "license": "BSD-3-Clause",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "~11.0.0",
    "@commitlint/config-conventional": "~11.0.0",
    "@rollup/plugin-json": "~4.1.0",
    "@rollup/plugin-node-resolve": "~9.0.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "~4.4.0",
    "@typescript-eslint/parser": "~4.4.0",
    "eslint": "~7.10.0",
    "husky": "~4.3.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^26.5.2",
    "rollup": "~2.29.0",
    "rollup-plugin-node-builtins": "~2.1.2",
    "rollup-plugin-terser": "~7.0.2",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "saber", "fetch", "request"
  ],
  "files": [
    "lib",
    "esm",
    "dist",
    "types"
  ]
}
